FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       fixedNetFoam;

startFrom         startTime;

startTime         0;

stopAt            endTime;

endTime           1600;

deltaT            1;

writeControl      adjustableRunTime;

writeInterval     10;

purgeWrite        0;

writeFormat       ascii;

writePrecision    6;

writeCompression  uncompressed;

timeFormat        general;

timePrecision     4;

graphFormat       raw;

// Other setting
runTimeModifiable yes;

adjustTimeStep    yes;

maxCo             0.5;

